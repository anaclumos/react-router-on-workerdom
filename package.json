{
  "name": "tutorial",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf ./dist && rm -rf ./workerbuild",
    "nuke": "yarn clean && rm -rf ./node_modules",
    "buildw2w": "cd webapp2worker && deno compile --allow-read cli.ts  && cd ..",
    "build": "vite build",
    "w2w": "mkdir -p workerbuild && ./webapp2worker/webapp2worker ./dist/index.html > ./workerbuild/index.js && cp index.html ./workerbuild/index.html",
    "serve": "open http://127.0.0.1:8080/ && http-server ./workerbuild/",
    "start": "yarn clean && yarn && yarn buildw2w && yarn build && yarn w2w && yarn serve"
  },
  "dependencies": {
    "@ampproject/worker-dom": "^0.34.0",
    "history": "^5.3.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "6.0.0-beta.8"
  },
  "devDependencies": {
    "@rollup/plugin-replace": "^2.2.1",
    "@types/node": "14.x",
    "@types/react": "^17.0.19",
    "@types/react-dom": "^17.0.9",
    "@vitejs/plugin-react": "^1.0.1",
    "typescript": "^4.3.5",
    "vite": "^2.5.0"
  }
}